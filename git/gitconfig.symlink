# Local/private config goes in the include
[include]
	path = ~/.gitconfig.local
[push]
	default = simple
[pull]
	ff = only
[core]
	editor = vim
	excludesfile = ~/.gitignore
	autocrlf = input
[commit]
	gpgsign = true
[alias]
	co = checkout
	st = status
	lg1 = log --oneline
	s = switch
[gpg]
	program = /usr/local/bin/gpg
[color]
	ui = auto
